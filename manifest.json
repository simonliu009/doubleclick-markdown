{
  "manifest_version": 3,
  "name": "双击复制标签信息",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "tabs",
    "scripting",
    "notifications",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "options_page": "options.html"
} 